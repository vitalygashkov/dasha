{
  "name": "dasha",
  "version": "3.1.3",
  "description": "Streaming manifest parser",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "test": "npm run lint && npm run types && node --test",
    "types": "tsc -p tsconfig.json",
    "lint": "eslint . && prettier --check .",
    "fix": "eslint . --fix && prettier --write .",
    "build": "tsup dasha.js --format esm,cjs",
    "prepublishOnly": "npm run build"
  },
  "main": "./dist/dasha.js",
  "module": "./dist/dasha.mjs",
  "types": "./types/dasha.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitalygashkov/dasha"
  },
  "keywords": [
    "mpeg",
    "dash",
    "hls",
    "adaptive",
    "mpd",
    "m3u8",
    "manifest",
    "playlist"
  ],
  "author": "Vitaly Gashkov <vitalygashkov@vk.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/vitalygashkov/dasha/issues",
    "email": "vitalygashkov@vk.com"
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://boosty.to/vitalygashkov"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/vitalygashkov"
    }
  ],
  "engines": {
    "node": "20 || 21 || 22"
  },
  "dependencies": {
    "m3u8-parser": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.10.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "prettier": "^3.4.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
